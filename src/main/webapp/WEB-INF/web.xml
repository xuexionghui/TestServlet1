<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <!-- servlet的内部名称 ，可以自定义-->
    <servlet-name>HelloServlet</servlet-name>
    <!-- servlet类名： 包名+简单类名-->
    <servlet-class>com.junlaninfo.HelloServlet</servlet-class>
  </servlet>
  <servlet>
    <!-- servlet的内部名称 ，可以自定义-->
    <servlet-name>LifeCycleServlet</servlet-name>
    <!-- servlet类名： 包名+简单类名-->
    <servlet-class>com.junlaninfo.LifeCycleServlet</servlet-class>
  </servlet>
  <servlet>
    <!-- servlet的内部名称 ，可以自定义-->
    <servlet-name>ServetlDemo4</servlet-name>
    <!-- servlet类名： 包名+简单类名-->
    <servlet-class>com.junlaninfo.ServetlDemo4</servlet-class>
  </servlet>

  <servlet-mapping>
    <!-- servlet的内部名称，和上面的名称保持一致！！！-->
    <servlet-name>HelloServlet</servlet-name>
    <!-- servlet的访问名称： /名称 -->
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  <!-- servlet的内部名称，和上面的名称保持一致！！！-->
  <servlet-name>LifeCycleServlet</servlet-name>
  <!-- servlet的访问名称： /名称 -->
  <url-pattern>/life</url-pattern>
</servlet-mapping>
  <servlet-mapping>
    <!-- servlet的内部名称，和上面的名称保持一致！！！-->
    <servlet-name>ServetlDemo4</servlet-name>
    <!-- servlet的访问名称： /名称 -->
    <url-pattern>/demo</url-pattern>
  </servlet-mapping>
</web-app>
